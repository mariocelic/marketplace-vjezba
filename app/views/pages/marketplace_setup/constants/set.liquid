---
method: post
---
{% liquid
  function current_profile = 'lib/current_profile', user_id: context.current_user.id
  include 'lib/must_be_superadmin', profile: current_profile
  function object = 'lib/commands/constants/set', name: context.params.name, value: context.params.value

  if object.valid
    redirect_to '/marketplace_setup'
  else
    echo context.constants["USE_WEBSOCKET_CHAT"]
    echo context.params
    echo object
    break
  endif
%}
