{% liquid
  function current_profile = 'lib/current_profile', user_id: context.current_user.id
  include 'lib/profile_present_or_redirect', profile: current_profile, require_profile: false
  include 'lib/must_be_superadmin', profile: current_profile

  function list = 'lib/queries/items/search', limit: 200

  include 'theme/simple/admin/menu', section: 'items'
  include 'theme/simple/admin/items/index', items: list, role: 'admin'
%}
