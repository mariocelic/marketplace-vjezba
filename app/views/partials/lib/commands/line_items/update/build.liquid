{% liquid
  assign quantity = params.quantity | plus: 0
%}
{% parse_json object %}
  {
    "id": {{ object.id | json }},
    "quantity": {{ quantity | json }},
    "total_price_cents": {{ object.price_cents | times: 1 | times: quantity | json }}
  }
{% endparse_json %}

{% liquid
  return object
%}
